  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/apps/front-end/test/setupJestDom.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "jest-environment-jsdom",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/iam(|/.*)$": "<rootDir>/libs/iam/src/$1",
      "^@app/neo4j(|/.*)$": "<rootDir>/libs/neo4j/src/$1",
      "^libs/(.*)$": "<rootDir>/libs/$1"

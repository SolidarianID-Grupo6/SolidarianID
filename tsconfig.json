{
  "compilerOptions": {
    "module": "commonjs",
    "allowJs": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "typeRoots": ["node_modules/@types"],
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "moduleResolution": "node",
    "paths": {
      "@app/iam": [
        "libs/iam/src"
      ],
      "@app/iam/*": [
        "libs/iam/src/*"
      ],
      "@app/neo4j": [
        "libs/neo4j/src"
      ],
      "@app/neo4j/*": [
        "libs/neo4j/src/*"
      ]
    }
  }
}